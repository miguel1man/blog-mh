{
  "$schema": ".app/app.schema.json",
  "title": "MH",
  "description": "Blog personal.",
  "editThisNote": {
    "url": "https://github.com/miguel1man/blog-mh/edit/{{branch}}/{{file}}"
  },
  "customProperties": {
    "properties": [
      {
        "path": "props",
        "options": {
          "date": {
            "locale": "en-US"
          }
        }
      }
    ]
  },
  "sidebar": {
    "links": [
      {
        "url": "https://github.com/miguel1man/blog-mh",
        "label": "GitHub",
        "icon": "github"
      }
    ],
    "notes": [
      {
        "pattern": "^/[^/]+$"
      }
    ]
  }
}
